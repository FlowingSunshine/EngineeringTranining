<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!--<style name="AppTheme" parent="android:Theme.Holo.Light">-->
    <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="actionBarTabStyle">@style/ClubToolbar</item>
    </style>

    <style name="TextLeftActionbar">
        <item name="android:textSize">@dimen/text_size_actionbar_left</item>
        <item name="android:textColor">@color/ColorActionBarTitleBase</item>
    </style>

    <style name="ClubToolbar" parent="Widget.AppCompat.ActionBar">
        <item name="contentInsetStart">0dp</item><!-- 设置该属性解决空白部分-->
        <item name="contentInsetEnd">0dp</item>
    </style>
    <style name="ActionBarTabText">
        <item name="android:textSize">@dimen/text_size_tab_actionbar</item>
    </style>

</resources>
